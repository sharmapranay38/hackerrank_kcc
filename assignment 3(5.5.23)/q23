//WAP to find the minimum and maximum element of the array.

#include<bits/stdc++.h>
using namespace std;
int main(){
    // vector<int> ;
    int a[5] = {10,20,30,40,50};
    int max=a[0],min=a[0];
    for(int i=0;i<5;i++){
        if(a[i]>max) max = a[i];
        else if(a[i]<min) min = a[i];
    }
    cout<<"max is "<<max<<" min is "<<min;

    return 0;
}
