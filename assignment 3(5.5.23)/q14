#include<bits/stdc++.h>
using namespace std;
int fact(int n){
    if(n<=1) return 1;
    else return n*fact(n-1);
}
int main(){
    int sum_even=0,sum_odd=0,n;
    cin>>n;
    for(int i =0;i<=n;i++){
        if(i%2==0) sum_even+=i;
        else sum_odd+=i;
    }
    cout<<"sum of even is "<<sum_even<<endl;
    cout<<"sum of oddd is "<<sum_odd;
    return 0;

}


